<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="CustomWindowTitleText" parent="android:TextAppearance.WindowTitle">
		<item name="android:textSize">20dip</item>
		<item name="android:textColor">#FFFFFF</item>
		<item name="android:textStyle">bold</item>
		<item name="android:text">hello</item>
		<item name="android:gravity">center_vertical|center_horizontal</item>
		<item name="android:layout_x">200px</item>

	</style>
	<style name="CustomWindowTitleBackground">
		<item name="android:background">#ff336600</item>

	</style>
	<style name="CustomWindowTitle" parent="android:WindowTitle">
		<item name="android:textAppearance">@style/CustomWindowTitleText</item>
		<item name="android:shadowDx">20</item>
		<item name="android:shadowDy">0</item>
		<item name="android:shadowRadius">5</item>

		<item name="android:shadowColor">#768BA7</item>

	</style>
	<style name="CustomTheme" parent="android:Theme">
		<item name="android:windowTitleSize">40dip</item>
		<item name="android:windowTitleStyle">@style/CustomWindowTitle</item>
		<item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
	</style>

	<color name="output_green">#ff339900</color>
	<color name="black">#000000</color>
	<color name="button_red">#ff009900</color>
	<color name="solid_green">#21721A</color>
	<color name="list_test_green">#316500</color>
	<drawable name="green">#EEFCD1</drawable>
	<drawable name="dark_green">#B7CE8C</drawable>




</resources>